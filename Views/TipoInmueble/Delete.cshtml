@model InmobiliariaWebApp.Models.TipoInmueble

@{
    ViewData["Title"] = "Eliminar Tipo de Inmueble";
}

<h1>Eliminar Tipo de Inmueble</h1>
<h3>¿Estás seguro de que quieres eliminar este tipo?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">@Html.DisplayNameFor(model => model.Nombre)</dt>
        <dd class = "col-sm-10">@Html.DisplayFor(model => model.Nombre)</dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver al Listado</a>
    </form>
</div>